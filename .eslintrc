{
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "semi": [
            2,
            "always"
        ],
        "no-console": 0
    }
}